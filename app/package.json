{
    "name": "rise-focus",
    "description": "oclif example Hello World CLI",
    "version": "0.0.1",
    "author": "dodgeblaster",
    "bin": {
        "focus": "./bin/run"
    },
    "bugs": "https://github.com/dodgeblaster/rise-cli/issues",
    "dependencies": {
        "@oclif/core": "^1",
        "@oclif/plugin-help": "^5",
        "@oclif/plugin-plugins": "^2.0.1",
        "aws-sdk": "^2.1065.0",
        "axios": "^0.25.0",
        "rise-cli-foundation": "^0.0.3",
        "rise-foundation": "^0.0.41"
    },
    "devDependencies": {
        "@oclif/test": "^2",
        "@types/chai": "^4",
        "@types/jest": "^27.4.0",
        "@types/node": "^16.11.21",
        "chai": "^4",
        "eslint": "^7.32.0",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1.0.2",
        "globby": "^11",
        "jest": "^27.4.7",
        "oclif": "^2",
        "shx": "^0.3.3",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.2.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/dodgeblaster/rise-cli",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "dist/index.js",
    "oclif": {
        "bin": "focus",
        "dirname": "oex",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            }
        }
    },
    "repository": "dodgeblaster/rise-cli",
    "scripts": {
        "build": "shx rm -rf dist && tsc -b",
        "lint": "echo \"no linting\"",
        "postpack": "shx rm -f oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "test": "mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif readme && git add README.md",
        "ci:test": "CI=true ./node_modules/.bin/jest",
        "local:test": "./node_modules/.bin/jest --coverage",
        "dev:test": "jest --watch --maxWorkers=1 $1"
    },
    "types": "dist/index.d.ts"
}
